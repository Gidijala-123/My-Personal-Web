    .back .content{
    position:absolute;
    top:10px;
    left:10px;
    right:10px;
    bottom:10px;
    padding-left:10px;
    border:2px solid aqua;
    border-radius: 0 0 50px 0px;
    text-align:left;
    font-size:10px;
    white-space:nowrap;
    line-height:10px;
    box-shadow:0 5px 10px rgba(0,0,0,.5);
    }
    .back .content h1{
    color:#fff;
    font-size:30px;
    margin:0 0 10px;
    padding:0;
    }
    .back .content p{
    color:#fff;
    }
    .back span{
    z-index:-1;
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    display:block;
    box-sizing:border-box;
    }
    .back span:nth-child(1){
    transform:rotate(0deg);
    }
    .back span:nth-child(2){
    transform:rotate(90deg);
    }
    .back span:nth-child(3){
    transform:rotate(180deg);
    }
    .back span:nth-child(4){
    transform:rotate(270deg);
    }
    .back span:nth-child(2):before{
    animation-delay:-2s;
    }
    .back span:nth-child(4):before{
    animation-delay:-2s;
    }
    .back span:before{
    content:'';
    position:absolute;
    width:100%;
    height:2px;
    background:aqua;
    animation: animate 4s linear infinite;
    }
    @keyframes animate{
    0%{
    transform:scaleX(0);
    transform-origin:left;
    }
    50%{
    transform:scaleX(1);
    transform-origin:left;
    }
    50.1%{
    transform:scaleX(1);
    transform-origin:right;
    }
    100%{
    transform:scaleX(0);
    transform-origin:right;
    }
    }
